#include <chrono>
#include <iostream>
#include <future>
using namespace std;

void timer() {
    cout << "Start\n";
    for(int i=0;i<20;i++)
    {
        cout << (i) << '\n';
        this_thread::sleep_for(chrono::seconds(1));
    }
    cout << "DONE\n";
}
int main ()
{
    auto future = async(timer);
    //std::cout << "test\n";
}
